{
  "extends": "solhint:recommended",
  "rules": {
    "func-order": "off",
    "mark-callable-contracts": "off",
    "no-empty-blocks": "off",
    "compiler-version": "off",
    "private-vars-leading-underscore": "error",
    "reason-string": "off",
    "reentrancy": "warn",
    "func-visibility": "error",
    "indent": "off",
    "not-rely-on-time": "off",
    "bracket-align": "off"
  }
}